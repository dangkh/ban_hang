<div class="container">
  <div class="row">
    <div style="width: 50%" class="col-md-5">
      <div id="hcontainer" data-chart="<%= @data %>"></div>
    </div>
    <div style="width: 50%; display: inline-block;">
      <div id="hhcontainer" data-chart="<%= @data1 %>"></div>
    </div>
  </div>

</div>
<div class="container product-border">
  <div class="pull-right">
    <button class="btn btn-warning">
      <%= link_to t(:download_excel), admin_statistics_url(format: "xls"), id: "word-li" %>
    </button>
  </div>
  <div class="table-responsive padding-table">
    <table class="table table-width">
      <thead>
        <tr>
          <th><%= t :image %></th>
          <th><%= t :name_product %></th>
          <th><%= t :sold_quantity %></th>
          <th><%= t :price %></th>
          <th><%= t :total_price %></th>
        </tr>
      </thead>
      <tbody>
        <% @products.each do |product, quantity| %>
          <tr>
            <td><%= picture_for product %></td>
            <td class="element-padding-showorder">
              <%= product.name %>
            </td>
            <td class="element-padding-showorder">
              <%= quantity %>
            </td>
            <td class="element-padding-showorder">
              <%= product.price %>
            </td>
            <td class="element-padding-showorder">
              <%= number_to_currency quantity * product.price %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <div class="text-center"><%= paginate @products %></div>
  </div>
</div>
<div class="container product-border">
  <div class="row">
    <div class="col-md-6">
    <div class="panel panel-default">
      <div class="panel-heading text-center"><%= link_to "User created order by time", admin_users_path %></div>
      <div class="panel-body">
        <%= render partial: "user1", collection: @user1s %>
      </div>
    </div>

    </div>
    <div class="col-md-6">
      <div class="panel panel-default">
        <div class="panel-heading text-center">User order by order number </div>
        <div class="panel-body">
          <table class="table">
            <thead>
              <tr>
                <th><%= t :email %></th>
                <th><%= t :price %></th>
              </tr>
            </thead>
            <tbody>
              <%= render partial: "user2", collection: @user2s %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
